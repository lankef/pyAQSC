             (($IS_INTEGER SIMP) ((MPLUS SIMP) ((MTIMES SIMP) -1. $I3) $N))
             (($IS_SMALLER SIMP) ((MPLUS SIMP) ((MTIMES SIMP) -1. $I3) $N) $I3)
             ((%SUM SIMP)
              ((MTIMES SIMP) ((|$B_alpha_coef| SIMP ARRAY) $I228)
               ((|$B_alpha_coef| SIMP ARRAY)
                ((MPLUS SIMP) ((MTIMES SIMP) -1. $I228) ((MTIMES SIMP) -1. $I3)
                 $N)))
              $I228 0. ((MPLUS SIMP) ((MTIMES SIMP) -1. $I3) $N)))
            $I3 (($CEILING SIMP) ((MTIMES SIMP) 0.5 $N)) (($FLOOR SIMP) $N)))) 
